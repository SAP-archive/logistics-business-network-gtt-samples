{
  "_version": "1.12.0",
  "sap.app": {
    "id": "com.sap.gtt.app.sample.pof",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "subTitle": "{{appSubTitle}}",
    "shortTitle": "",
    "info": "",
    "description": "{{appDescription}}",
    "tags": {
      "keywords": [],
      "technicalAttributes": []
    },
    "ach": "SCM-LBN-GTT-COR",
    "dataSources": {
      "mainService": {
        "uri": "trackpurchaseorders/sap/logistics/gtt/sample/pof/odata/v1/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml",
          "annotations": [
            "mainAnnotations",
            "localAnnotations"
          ],
          "maxAge": 360
        }
      },
      "mainAnnotations": {
        "uri": "trackpurchaseorders/sap/logistics/gtt/sample/pof/rest/v1/uiAnnotation",
        "type": "ODataAnnotation",
        "settings": {
          "localUri": "localService/annotations.xml"
        }
      },
      "localAnnotations": {
        "uri": "annotations/annotations.xml",
        "type": "ODataAnnotation"
      },
      "restService": {
        "uri": "trackpurchaseorders/sap/logistics/gtt/sample/pof/rest/v1",
        "type": "JSON",
        "settings": {
          "localUri": "localService/mockdata/",
          "maxAge": 30
        }
      }
    },
    "resources": "resources.json",
    "offline": false,
    "sourceTemplate": {
      "id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
      "version": "1.40.12"
    },
    "crossNavigation": {
      "inbounds": {
        "PurchaseOrder-track": {
          "semanticObject": "PurchaseOrder",
          "action": "track",
          "icon": "sap-icon://cart-full",
          "title": "{{appTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": [
      "sap_fiori_3",
      "sap_belize_hcb",
      "sap_belize_hcw"
    ],
    "fullWidth": true
  },
  "sap.ui5": {
    "flexEnabled": false,
    "resources": {
      "js": [],
      "css": [{
        "uri": "css/style.css",
        "id": "pof-css"
      }]
    },
    "dependencies": {
      "minUI5Version": "1.78.11",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.comp": {},
        "sap.ui.fl": {},
        "sap.ui.layout": {},
        "sap.ui.table": {},
        "sap.ui.vbm": {},
        "sap.ui.vk": {},
        "sap.f": {},
        "sap.uxap": {},
        "sap.suite.ui.commons": {},
        "sap.suite.ui.microchart": {}
      },
      "components": {}
    },
    "componentUsages": {},
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.sap.gtt.app.sample.pof.i18n.i18n"
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "trackpurchaseorders/sap/logistics/gtt/sample/pof/rest/v1/i18n/i18n.properties"
      },
      "": {
        "dataSource": "mainService",
        "settings": {
          "defaultCountMode": "Inline",
          "refreshAfterChange": true
        },
        "preload": true
      }
    },
    "rootView": {
      "viewName": "com.sap.gtt.app.sample.pof.view.App",
      "type": "XML",
      "id": "app",
      "async": true
    },
    "handleValidation": true,
    "config": {
      "css": {
        "id": "com.sap.gtt.app.sample.pof",
        "uri": "css/pof.less"
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "com.sap.gtt.app.sample.pof.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "bypassed": {
          "target": ["notFound"]
        },
        "async": true
      },
      "routes": [
        {
          "name": "purchaseOrderList",
          "pattern": "",
          "target": "purchaseOrderList"
        },
        {
          "name": "PurchaseOrderItemDetails",
          "pattern": "PurchaseOrderItem(guid'{id}')",
          "target": "PurchaseOrderItemDetails"
        },
        {
          "name": "PurchaseOrderDetails",
          "pattern": "PurchaseOrder(guid'{id}')",
          "target": "PurchaseOrderDetails"
        },
        {
          "name": "DeliveryItemDetails",
          "pattern": "DeliveryItem(guid'{id}')",
          "target": "DeliveryItemDetails"
        }
      ],
      "targets": {
        "purchaseOrderList": {
          "viewId": "purchaseOrderList",
          "viewType": "XML",
          "viewName": "PurchaseOrderList",
          "viewLevel": 0,
          "viewPath": "com.sap.gtt.app.sample.pof.view.list",
          "title": "{{appTitle}}"
        },
        "PurchaseOrderDetails": {
          "viewId": "PurchaseOrderDetails",
          "viewType": "XML",
          "viewName": "PurchaseOrderDetails",
          "viewLevel": 1,
          "viewPath": "com.sap.gtt.app.sample.pof.view.order",
          "title": "{{purchaseOrderDetailsViewTitle}}"
        },
        "DeliveryItemDetails": {
          "viewId": "DeliveryItemDetails",
          "viewType": "XML",
          "viewName": "DeliveryItemDetails",
          "viewLevel": 1,
          "viewPath": "com.sap.gtt.app.sample.pof.view.deliveryItem",
          "title": "{{deliveryItemDetailsViewTitle}}"
        },
        "PurchaseOrderItemDetails": {
          "viewId": "PurchaseOrderItemDetails",
          "viewType": "XML",
          "viewName": "POItemDetails",
          "viewLevel": 1,
          "viewPath": "com.sap.gtt.app.sample.pof.view.item",
          "title": "{{purchaseOrderItemDetailsViewTitle}}"
        },
        "notFound": {
          "viewId": "notFound",
          "viewType": "XML",
          "viewName": "NotFound",
          "viewLevel": 0,
          "title": "{{notFoundViewTitle}}"
        }
      }
    },
    "extends": {
      "extensions": {}
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "resourceRoots": {},
    "services": {
      "ShellUIService": {
        "factoryName": "sap.ushell.ui5service.ShellUIService",
        "lazy": false,
        "settings": {
          "setHierarchy": "auto",
          "setTitle": "auto"
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": ["F4036"],
    "archeType": "transactional"
  },
  "sap.platform.cf": {
    "oAuthScopes": [
      "$XSAPPNAME.pof.r"
    ]
  }
}
